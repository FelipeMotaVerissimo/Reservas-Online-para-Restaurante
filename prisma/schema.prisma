generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Reserva {
  id        Int      @id @default(autoincrement())
  nome      String
  email     String
  telefone  String
  data      DateTime
  pessoas   Int
  criadoEm  DateTime @default(now())
}

model Usuario {
  id    Int    @id @default(autoincrement())
  nome  String
  email String @unique
  senha String
}
